size (600, 600);
background (0);
int i = 0;
int i2 = 0;
int i3 = 0;

translate(300, 300);
fill(0, 0, 0, 0);
stroke(255);
strokeWeight(2);

circle(0, 0, 120);
while (i < 6) {
 rotate(PI/3);
 circle(0, -120, 120);
 i ++;
}

while(i2 < 6) {
  rotate(PI/3); 
circle(105, 0, 120);
i2 ++;
}

while(i3 < 6) {
  rotate(PI/3); 
circle(0, -60, 120);
i3 ++;
}










 
 
